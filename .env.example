# n8n configuration
N8N_PORT=5678
N8N_PROTOCOL=http
N8N_HOST=localhost
N8N_EDITOR_BASE_URL=http://localhost:5678
N8N_USER_FOLDER=/data

# Timezone
GENERIC_TIMEZONE=Europe/Moscow
TZ=Europe/Moscow

# Security (generate your own key!)
# N8N_ENCRYPTION_KEY=

# Optional: Basic auth
# N8N_BASIC_AUTH_ACTIVE=true
# N8N_BASIC_AUTH_USER=
# N8N_BASIC_AUTH_PASSWORD=
